{
    "exp_name": "train_goal_gen",
    "seed": 123,
    "batch_size": 4,
    "learning_rate": 8e-5,
    "min_lr_scale": 1.0,
    "warmup_steps": 800,
    "device": "cuda",
    "num_workers": 2,
    "save_epoch": 1,
    "pretrained_model_dir": "/app/resources/instruct-pix2pix",
    "encoder_dir": "/app/resources/t5-base",
    "data_dir": "/app/data/calvin_debug_dataset_format",
    "ckpt_root": "/app/results/goal_gen/checkpoints",
    "log_root": "/app/results/goal_gen/logs",
    "resume": null,
    "color_aug": false,
    "conditioning_dropout_prob": 0.05,
    "use_ema": false,
    "gradient_checkpointing": true,
    "adam_beta1": 0.95,
    "adam_beta2": 0.999,
    "adam_weight_decay": 1e-2,
    "adam_epsilon": 1e-08,
    "trainer": {
        "accelerator": "gpu",
        "strategy": "ddp",
        "precision": "bf16-mixed",
        "accumulate_grad_batches": 4,
        "logger": [
            "tensorboard"
        ],
        "use_distributed_sampler": true,
        "gradient_clip_val": 0.7,
        "log_every_n_steps": 50,
        "max_epochs": 50
    }
}